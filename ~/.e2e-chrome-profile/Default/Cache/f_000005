            issuer: 'https://dev-618888.okta.com/oauth2/default',
            clientId: '0oap4cr6z5SmMiqsh4x6'
        });
        this.isAuthenticated = new rxjs__WEBPACK_IMPORTED_MODULE_2__["BehaviorSubject"](false);
    }
    checkAuthenticated() {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const authenticated = yield this.authClient.session.exists();
            this.isAuthenticated.next(authenticated);
            return authenticated;
        });
    }
    login(username, password) {
        return Object(tslib__WEBPACK_IMPORTED_MODULE_0__["__awaiter"])(this, void 0, void 0, function* () {
            const transaction = yield this.authClient.signIn({ username, password });
            if (transaction.status !== 'SUCCESS') {
                console.log("Login error from server is ", transaction.status);
                throw Error('Login is not successful.');
            }
            this.isAuthenticated.next(true);
            this.authClient.session.setCookieAndRedirect(transaction.sessionToken);
        });
    }
    logout() {
        try {
            this.authClient.signOut();
            //  this.isAuthenticated.next(false);
        }
        catch (err) {
            console.error(err);
        }
    }
